@model List<FirstWebApplication.Models.User>
@{
    ViewBag.Title = "Lietotaju saraksts";
}

<h2>Lietotaji</h2>

<div>
    @Html.ActionLink("Pievienot jaunu", "Create", "User")
</div>
<table class="table table-striped">
    <tr>
        <th>Vards</th>
        <th>Uzvards</th>
        <th></th>
    </tr>
    @foreach (var user in Model)
    {
        <tr>
            <td>@user.FirstName</td>
            <td>@user.LastName</td>
            <td>@Html.ActionLink("Rediget", "Edit", "User", new { id = user.Id}, null)</td>
        </tr>
    }
</table>