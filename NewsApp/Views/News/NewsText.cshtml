@model NewsApp.Models.News
@{
    ViewBag.Title = "NewsText";
}

    <div class="container">
        <h2>@Model.Name</h2>
        <table>
            <tr>
                <td><h4>@Model.Author</h4></></td>
                <td><h4>@Model.Date.Value.ToString("dd/MM/yyyy")</h4></td>
            </tr>
        </table>
        <hr />
        <img src=@Model.Picture class="img-responsive" alt="Image">
        <hr />
        <p>@Model.Article</p>
    </div>