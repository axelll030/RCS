@model List<NewsApp.Models.Topic>
@{
    ViewBag.Title = "Topics";
}

<nav class="navbar navbar-default" id="navbar-topic">
    <div class="container-fluid">
        <ul class="nav navbar-nav" style="position:relative; left: -22px;">
            @foreach (var topic in Model)
            {
                <li id="topic-hover"><a href="#"><strong>@topic.Topics</strong></a></li>      
            }
        </ul>
    </div>
</nav>
<div class="container">
    <h2>ABOUT</h2>
</div>